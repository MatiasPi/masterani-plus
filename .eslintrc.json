{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module",
        "ecmaFeatures": {
            "modules": true
        }
    },
    "rules": {
        "no-console": "off",
        "no-empty": "off"
    },
    "env": {
        "browser": true,
        "webextensions": true
    }
}